<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeViewFragment" >

    <TextView
        android:id="@+id/barra_inferior_pg_5"
        android:layout_width="373dp"
        android:layout_height="110dp"
        android:background="@color/verdeapp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/profile_btn_pg_5"
        android:layout_width="63dp"
        android:layout_height="55dp"
        android:layout_marginStart="32dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        android:src="@drawable/icon_perfil"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/add_task_btn_pg_5"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/barra_inferior_pg_5"
        app:layout_constraintTop_toTopOf="@+id/barra_inferior_pg_5" />

    <ImageButton
        android:id="@+id/add_task_btn_pg_5"
        android:layout_width="73dp"
        android:layout_height="72dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        android:src="@drawable/icon_plus"
        app:layout_constraintBottom_toBottomOf="@+id/barra_inferior_pg_5"
        app:layout_constraintEnd_toEndOf="@+id/barra_inferior_pg_5"
        app:layout_constraintStart_toStartOf="@+id/barra_inferior_pg_5"
        app:layout_constraintTop_toTopOf="@+id/barra_inferior_pg_5" />

    <ImageButton
        android:id="@+id/calendar_btn_pg_5"
        android:layout_width="63dp"
        android:layout_height="55dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        android:src="@drawable/icon_calendar"
        app:layout_constraintBottom_toBottomOf="@+id/barra_inferior_pg_5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.676"
        app:layout_constraintStart_toEndOf="@+id/add_task_btn_pg_5"
        app:layout_constraintTop_toTopOf="@+id/barra_inferior_pg_5" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="376dp"
        android:layout_height="122dp"
        android:background="#00FFFFFF"
        android:scaleType="fitXY"
        android:src="@drawable/barra_ods_menu"
        app:layout_constraintBottom_toTopOf="@+id/barra_inferior_pg_5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.359" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="244dp"
        android:layout_height="50dp"
        android:fontFamily="@font/hammersmith_one"
        android:gravity="center"
        android:text="Salve o Mundo"
        android:textColor="@color/verdeapp"
        android:textSize="28sp"
        app:layout_constraintBottom_toTopOf="@+id/imageButton6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.086"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />
</androidx.constraintlayout.widget.ConstraintLayout>