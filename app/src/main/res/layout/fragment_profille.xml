<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfilleFragment" >

    <Button
        android:id="@+id/btn_logoff_pg_7"
        android:layout_width="226dp"
        android:layout_height="48dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="236dp"
        android:backgroundTint="#C2C2C2"
        android:fontFamily="@font/hammersmith_one"
        android:text="Logoff"
        android:textColor="@color/cinzabtn"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_ajuda_pg_7"
        app:layout_constraintVertical_bias="0.951"
        app:rippleColor="#FFFFFF" />

    <ImageView
        android:id="@+id/logoff_icon_pg_10"
        android:layout_width="31dp"
        android:layout_height="30dp"
        android:layout_marginStart="36dp"
        android:src="@drawable/icon_logoff_pg07__1_"
        app:layout_constraintBottom_toBottomOf="@+id/btn_logoff_pg_7"
        app:layout_constraintEnd_toStartOf="@+id/btn_logoff_pg_7"
        app:layout_constraintHorizontal_bias="0.412"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btn_logoff_pg_7" />

    <Button
        android:id="@+id/btn_ajuda_pg_7"
        android:layout_width="226dp"
        android:layout_height="48dp"
        android:layout_marginTop="216dp"
        android:layout_marginEnd="56dp"
        android:backgroundTint="#C2C2C2"
        android:fontFamily="@font/hammersmith_one"
        android:text="Ajuda e Feedback"
        android:textColor="@color/cinzabtn"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:rippleColor="#FFFFFF" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="401dp"
        android:layout_height="142dp"
        android:src="@drawable/backgoundzoom_vs2_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="49dp"
        android:layout_height="50dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="fitCenter"
        android:src="@drawable/seta_branca_pg_4"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.099"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/hammersmith_one"
        android:text="PERFIL"
        android:textColor="#FFFFFF"
        android:textSize="55dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="33dp"
        android:layout_height="28dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        android:src="@drawable/icon_ajuda_pg07__1_"
        app:layout_constraintBottom_toBottomOf="@+id/btn_ajuda_pg_7"
        app:layout_constraintEnd_toStartOf="@+id/btn_ajuda_pg_7"
        app:layout_constraintHorizontal_bias="0.64"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btn_ajuda_pg_7" />

</androidx.constraintlayout.widget.ConstraintLayout>